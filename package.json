{
  "name": "scrambles-matcher",
  "version": "0.1.0",
  "description": "Tool to make matching scrambles to results easier.",
  "keywords": [
    "speedcubing",
    "WCA",
    "competition",
    "results",
    "scrambles"
  ],
  "homepage": "https://viroulep.github.io/scrambles-matcher/",
  "author": "Philippe Virouleau",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/viroulep/scrambles-matcher"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.5.3",
    "xlsx": "^0.14.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
